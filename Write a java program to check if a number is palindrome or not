import java.util.Scanner;
public class lab4 {
public static void main(String[] args) {
Scanner input = new Scanner(System.input);
int i,j;
System.out.print("Enter the number: ");
int n = input.nextInt();
int sum = 0;
int r = 0;
int temp = n;
while(n&gt;0)
{
r=n%10;
sum = (sum*10)+r;
n=n/10;
}
if(temp==sum)
System.out.print("Palindrome");
else
System.out.print("Not Palindrome");
}
}
